{
  "name": "prod_aic_calendar_to_lessonmgmt_scheduler_sink_connector",
  "config": {
    "connector.class": "io.confluent.connect.jdbc.JdbcSinkConnector",
    "tasks.max": "1",
    "topics": "prod.aic.calendar.public.scheduler",
    "connection.url": "${file:/config/kafka-connect-config.properties:lessonmgmt_url}",
    "auto.create": "false",
    "insert.mode": "upsert",
    "table.name.format": "scheduler",
    "fields.whitelist": "created_at,deleted_at,end_date,freq,resource_path,scheduler_id,start_date,updated_at",
    "transforms": "unwrap,route",
    "transforms.route.type": "org.apache.kafka.connect.transforms.RegexRouter",
    "transforms.route.regex": "([^.]+).([^.]+).([^.]+).([^.]+).([^.]+)",
    "transforms.route.replacement": "$5",
    "transforms.unwrap.type": "io.debezium.transforms.ExtractNewRecordState",
    "delete.enabled": "false",
    "pk.mode": "record_value",
    "pk.fields": "scheduler_id",
    "transforms.unwrap.drop.tombstones": "true",
    "transforms.unwrap.delete.handling.mode": "drop"
  }
}